  {
    "problemMatchers": [
      {
        "name": "%task.meteor.problemMatcher%",
        "source": "%extension.displayName%",
        "owner": "%extension.displayName%",
        "pattern": {
          "regexp": "^(.*.xaml)\\((\\d+),(\\d+)\\):.*?(error|warning)\\s(.*?):\\s(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      }
    ]
  }